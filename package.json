{
   "name": "ant-design-pro",
   "version": "5.0.0",
   "private": true,
   "description": "An out-of-box UI solution for enterprise applications",
   "scripts": {
      "analyze": "cross-env ANALYZE=1 umi build",
      "build": "umi build",
      "deploy": "npm run build && npm run gh-pages",
      "dev": "npm run start:dev",
      "gh-pages": "gh-pages -d dist",
      "i18n-remove": "pro i18n-remove --locale=zh-CN --write",
      "postinstall": "umi g tmp",
      "lint": "umi g tmp && npm run lint:js && npm run lint:style && npm run lint:prettier",
      "lint-staged": "lint-staged",
      "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
      "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src && npm run lint:style",
      "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
      "lint:prettier": "prettier -c --write \"src/**/*\" --end-of-line auto",
      "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
      "openapi": "umi openapi",
      "precommit": "lint-staged",
      "prettier": "prettier -c --write \"src/**/*\"",
      "start": "cross-env UMI_ENV=dev HTTPS=1 umi dev --port=8800",
      "start:zwh": "cross-env zwh_test=spp  umi dev --port=8800",
      "start:dev": "cross-env REACT_APP_ENV=dev MOCK=none UMI_ENV=dev umi dev --port=8800",
      "start:no-mock": "cross-env MOCK=none UMI_ENV=dev umi dev",
      "start:no-ui": "cross-env UMI_UI=none UMI_ENV=dev umi dev",
      "start:pre": "cross-env REACT_APP_ENV=pre UMI_ENV=dev umi dev",
      "start:test": "cross-env REACT_APP_ENV=test MOCK=none UMI_ENV=dev umi dev",
      "pretest": "node ./tests/beforeTest",
      "test": "umi test",
      "test:all": "node ./tests/run-tests.js",
      "test:component": "umi test ./src/components",
      "serve": "umi-serve",
      "tsc": "tsc --noEmit"
   },
   "lint-staged": {
      "**/*.less": "stylelint --syntax less",
      "**/*.{js,jsx,ts,tsx}": "npm run lint-staged:js",
      "**/*.{js,jsx,tsx,ts,less,md,json}": [
         "prettier --write"
      ]
   },
   "browserslist": [
      "> 1%",
      "last 2 versions",
      "not ie <= 10"
   ],
   "dependencies": {
      "@ahooksjs/use-url-state": "^3.5.1",
      "@ant-design/icons": "^4.5.0",
      "@ant-design/pro-components": "^1.1.15",
      "@ant-design/pro-descriptions": "^1.6.8",
      "@ant-design/pro-form": "^1.18.3",
      "@ant-design/pro-layout": "^6.15.3",
      "@ant-design/pro-table": "^2.30.8",
      "@antv/x6": "^1.34.5",
      "@antv/x6-react-shape": "^1.6.3",
      "@antv/xflow": "^1.0.52",
      "@nutui/nutui-react": "^1.2.2",
      "@umijs/route-utils": "^1.0.36",
      "ace-builds": "^1.13.1",
      "ahooks": "^3.7.4",
      "antd": "^4.24.7",
      "antd-img-crop": "^4.5.2",
      "classnames": "^2.2.6",
      "file-saver": "^2.0.5",
      "jszip": "^3.7.1",
      "lodash": "^4.17.11",
      "moment": "^2.29.1",
      "nanoid": "^4.0.0",
      "omit.js": "^2.0.2",
      "pubsub-js": "^1.9.4",
      "qrcode": "^1.4.4",
      "react": "^17.0.0",
      "react-ace": "^10.1.0",
      "react-beautiful-dnd": "^13.1.1",
      "react-color": "^2.19.3",
      "react-copy-to-clipboard": "^5.0.4",
      "react-csv": "^2.0.3",
      "react-dev-inspector": "^1.1.1",
      "react-dom": "^17.0.0",
      "react-helmet-async": "^1.0.4",
      "react-infinite-scroll-component": "^6.1.0",
      "react-rte": "^0.16.3",
      "umi": "^3.5.0",
      "umi-serve": "^1.9.10",
      "urlsafe-base64": "^1.0.0"
   },
   "devDependencies": {
      "@ant-design/pro-cli": "^2.0.2",
      "@babel/core": "^7.21.4",
      "@babel/preset-env": "^7.21.4",
      "@types/express": "^4.17.0",
      "@types/file-saver": "^2.0.3",
      "@types/history": "^4.7.2",
      "@types/jest": "^26.0.0",
      "@types/lodash": "^4.14.144",
      "@types/qrcode": "^1.4.1",
      "@types/react": "^17.0.0",
      "@types/react-color": "^3.0.5",
      "@types/react-copy-to-clipboard": "^5.0.2",
      "@types/react-csv": "^1.1.2",
      "@types/react-dom": "^17.0.0",
      "@types/react-helmet": "^6.1.0",
      "@umijs/fabric": "^2.6.2",
      "@umijs/openapi": "^1.7.1",
      "@umijs/plugin-blocks": "^2.0.5",
      "@umijs/plugin-esbuild": "^1.0.1",
      "@umijs/plugin-openapi": "^1.2.0",
      "@umijs/preset-ant-design-pro": "^1.2.0",
      "@umijs/preset-dumi": "^1.1.7",
      "@umijs/preset-react": "^1.8.17",
      "@umijs/preset-ui": "^2.2.9",
      "@umijs/yorkie": "^2.0.3",
      "babel-loader": "^9.1.2",
      "carlo": "^0.9.46",
      "compression-webpack-plugin": "^10.0.0",
      "cross-env": "^7.0.0",
      "cross-port-killer": "^1.1.1",
      "detect-installer": "^1.0.1",
      "enzyme": "^3.11.0",
      "eslint": "^7.1.0",
      "express": "^4.17.1",
      "gh-pages": "^3.0.0",
      "jsdom-global": "^3.0.2",
      "lint-staged": "^10.0.0",
      "mockjs": "^1.0.1-beta3",
      "prettier": "^2.3.2",
      "puppeteer-core": "^8.0.0",
      "stylelint": "^13.0.0",
      "typescript": "^4.2.2"
   },
   "engines": {
      "node": ">=10.0.0"
   },
   "gitHooks": {
      "commit-msg": "fabric verify-commit"
   }
}
